<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.school.controller.DashboardController"
            stylesheets="@../css/dashboard.css"
            prefHeight="400.0" prefWidth="600.0">

    <top>
        <HBox fx:id="navbar" alignment="CENTER_RIGHT" spacing="10" >
            <padding>
                <Insets top="10" right="10" bottom="10" left="10"/>
            </padding>
            <Button fx:id="btnLogout"
                    text="Logout"

                    styleClass="btn-logout"/>
        </HBox>
    </top>

    <center>
        <HBox fx:id="navButtons" alignment="CENTER" spacing="30">
            <Button fx:id="btnAlumno" text="Alumno" onAction="#onAlumnos"  styleClass="nav-btn">

            </Button>
            <Button fx:id="btnCarrera" text="Carrera"  styleClass="nav-btn">

            </Button>
            <Button fx:id="btnAsignatura" text="Asignatura"  styleClass="nav-btn">

            </Button>

            <Button fx:id="btnPhoto" text="Tomar Foto"  styleClass="nav-btn" onAction="#onACamera">

            </Button>

        </HBox>
    </center>

</BorderPane>
